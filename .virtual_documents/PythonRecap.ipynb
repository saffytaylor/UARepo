def tempConv (fahrenheit):
    result = ((fahrenheit-32)*5)/9
    return result

fahrenheit = float(input("Please enter the temperature in fahrenheit: "))
celsius = tempConv(fahrenheit)
print("The temperature in Celsius is ",round(celsius,1)," degrees.")
